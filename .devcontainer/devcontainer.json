{
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-toolsai.jupyter",
        "ms-python.python",
        "denoland.vscode-deno",
        "esbenp.prettier-vscode"
      ],
      "settings": {
        "deno.enable": true,
        "deno.lint": true,
        "editor.formatOnSave": true,
        "python.defaultInterpreterPath": "/usr/bin/python3",
        "terminal.integrated.defaultProfile.linux": "bash",
        "jupyter.notebook.defaultKernel": "deno"
      }
    }
  },
  "postCreateCommand": "npm install -g deno"
}
